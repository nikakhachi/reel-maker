{
    "containerDefinitions": [
        {
            "name": "server",
            "image": "037049504876.dkr.ecr.eu-west-1.amazonaws.com/server:latest",
            "cpu": 0,
            "memoryReservation": 256,
            "portMappings": [
                {
                    "containerPort": 5000,
                    "hostPort": 0,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "PORT",
                    "value": "5000"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/server-task",
                    "awslogs-region": "eu-west-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "family": "server-task",
    "networkMode": "bridge",
    "volumes": [],
  

    "placementConstraints": []
 

   

}
